{
  "routes": [
    {
      "src": "^/sitemap\\.xml$",
      "dest": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "^/robots\\.txt$",
      "dest": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "^/manifest\\.json$",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "^/favicon\\.ico$",
      "dest": "/favicon.ico",
      "headers": {
        "Content-Type": "image/x-icon"
      }
    }
  ]
}